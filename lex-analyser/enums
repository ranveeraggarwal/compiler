        enum Tokens{IDENTIFIER=0x100,
                    NUMBER, 
                    NOT_OP,
                    ASS_OP,
                    ADD_OP,
                    SUB_OP,
                    MUL_OP,
                    DIV_OP,
                    LT_OP,
                    GL_OP,
                    LE_OP,
                    GE_OP,
                    EQ_OP,
                    NE_OP,
                    AND_OP,
                    OR_OP,
                    INC_OP,
                    STRING_LITERAL,
                    VOID,
                    INT,
                    FLOAT,
                    RETURN,
                    IF,
                    ELSE,
                    WHILE,
                    FOR,
                    INT_CONSTANT,
                    FLOAT_CONSTANT};
